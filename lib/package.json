{
  "name": "@bmthd/lift",
  "version": "0.2.1",
  "description": "React component for headings",
  "keywords": [
    "react",
    "typescript"
  ],
  "author": "bmthd <jougennotuki67@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/bmthd/lift"
  },
  "homepage": "https://github.com/bmthd/lift",
  "type": "module",
  "files": [
    "/dist"
  ],
  "main": "./dist/cjs/index.cjs",
  "module": "./dist/esm/index.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.cjs"
    }
  },
  "sideEffects": false,
  "scripts": {
    "dev": "vite",
    "build": "tsdown",
    "format:test": "biome format ./src",
    "format:fix": "biome format --write ./src",
    "lint:test": "biome lint ./src",
    "lint:safe-fix": "biome check --apply ./src",
    "lint:fix": "biome check --apply-unsafe ./src",
    "test": "vitest",
    "typecheck": "bunx @typescript/native-preview --noemit",
    "prepublishOnly": "npm run build"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.2.4",
    "@testing-library/jest-dom": "^6.4.5",
    "@testing-library/react": "^16.0.0",
    "@types/node": "^22.0.0",
    "@types/react": "19.2.1",
    "@vitejs/plugin-react": "^5.0.0",
    "happy-dom": "^18.0.0",
    "tsdown": "^0.17.0-beta.6",
    "typescript": "^5.4.5",
    "vite": "^7.0.0",
    "vite-plugin-dts": "^4.0.0",
    "vitest": "^4.0.0"
  },
  "peerDependencies": {
    "react": ">=17.0.0",
    "@types/react": ">=17.0.0"
  }
}
